<div class="contents row">
  <div class="container">
    <h2>Sign up</h2>
    <%= form_with model: @user, url: user_registration_path, id: 'new_user', class: 'new_user', local: true do |f| %>
      <%= devise_error_messages! %>

      <div class="field">
        <%= f.label :name %><br />
        <%= f.text_field :name %>
      </div>

      <div class="field">
        <%= f.label :email %><br />
        <%= f.email_field :email %>
      </div>

      <div class="field">
        <%= f.label :password %>
        <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <div class="field">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" %>
      </div>

      <%if @sns_id.present? %>
        <%= hidden_field_tag :sns_auth, true %>
      <% else %>
        <div class="field">
          <%= f.label :password %>
          <% @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <br />
          <%= f.password_field :password, autocomplete: "new-password" %>
        </div>

        <div class="field">
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
        </div>
      <% end %>

      

      
      <%= link_to user_twitter_omniauth_authorize_path, method: :post, class: 'btn' do %>
        <img class="img" src="https://qiita-user-contents.imgix.net/https%3A%2F%2Fqiita-image-store.s3.ap-northeast-1.amazonaws.com%2F0%2F492105%2Fb09823be-2a7d-cdd2-83ef-a48fe005ddbc.png?ixlib=rb-1.2.2&amp;auto=format&amp;gif-q=60&amp;q=75&amp;s=43bb61a706a5175db4d67422db3bdfc2" alt="image.png" data-canonical-src="https://qiita-image-store.s3.ap-northeast-1.amazonaws.com/0/492105/b09823be-2a7d-cdd2-83ef-a48fe005ddbc.png" srcset="https://qiita-user-contents.imgix.net/https%3A%2F%2Fqiita-image-store.s3.ap-northeast-1.amazonaws.com%2F0%2F492105%2Fb09823be-2a7d-cdd2-83ef-a48fe005ddbc.png?ixlib=rb-1.2.2&amp;auto=format&amp;gif-q=60&amp;q=75&amp;w=1400&amp;fit=max&amp;s=9639b5b3a5bcdc8c4503eb29587ecd92 1x" loading="lazy">
      <% end %>
      <P></p>
      <%= link_to user_facebook_omniauth_authorize_path, method: :post, class: 'btn' do%>
        <img class="img" src="https://scontent-sjc3-1.xx.fbcdn.net/v/t39.2365-6/17639236_1785253958471956_282550797298827264_n.png?_nc_cat=105&amp;ccb=1-3&amp;_nc_sid=ad8a9d&amp;_nc_ohc=oGgyl1CgWTwAX-kcro8&amp;_nc_ht=scontent-sjc3-1.xx&amp;oh=53b9f19e3143abd18f660eaeb7140a7e&amp;oe=607E2D16" width="200" alt="">
      <% end %>
      <P></p>
      <%= link_to user_google_oauth2_omniauth_authorize_path, method: :post, class: 'btn' do%>
        <%= image_tag 'btn_google_signin_light_pressed_web.png' %>
      <% end %>
      

      <div class="actions">
        <%= f.submit "Sign up" %>
      </div>
    <% end %>
  </div>
</div>
